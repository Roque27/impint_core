[
  // *************** TABLA PERSONAS ***************
  {
    "Key": "Personas",
    "From": "FROM PERSONAS P INNER JOIN CALLES C ON (C.AGF_CODIGO = P.AGF_CODIGO AND C.CLL_CODIGO = P.CLL_CODIGO) ",
    "Where": "WHERE P.PRS_NUMERO = {$prs_num}",
    "Parametros": [ { "Parametro": "prs_num", "Tipo": "Int32" } ],
    "Columnas": [
      {
        "Columna": "PRS_NUMERO",
        "Expresion": "P.PRS_NUMERO AS PRS_NUMERO",
        "Metodo": ""
      },
      {
        "Columna": "PRS_RAZON_SOCIAL",
        "Expresion": "P.PRS_RAZON_SOCIAL",
        "Metodo": ""
      },
      {
        "Columna": "PRS_CUIT",
        "Expresion": "TO_CHAR(COALESCE(NVL(P.PRS_CUIT, 0), 0)) AS PRS_CUIT",
        "Metodo": ""
      },
      {
        "Columna": "PRS_TIPO_DOC",
        "Expresion": "COALESCE(NVL(P.PRS_TIPO_DOC, ''), '0') AS PRS_TIPO_DOC",
        "Metodo": ""
      },
      {
        "Columna": "PRS_DOCUMENTO",
        "Expresion": "COALESCE(NVL(P.PRS_DOCUMENTO, 0), 0) AS PRS_DOCUMENTO",
        "Metodo": ""
      },
      {
        "Columna": "PRS_NRO",
        "Expresion": "COALESCE(NVL(P.PRS_NRO, 0), '0') AS PRS_NRO",
        "Metodo": ""
      },
      {
        "Columna": "PRS_DEPTO",
        "Expresion": "COALESCE(NVL(P.PRS_DEPTO, ''), '0') AS PRS_DEPTO",
        "Metodo": ""
      },
      {
        "Columna": "AGF_CODIGO",
        "Expresion": "NVL(P.AGF_CODIGO, 0) AS AGF_CODIGO",
        "Metodo": ""
      },
      {
        "Columna": "PRS_C_POSTAL",
        "Expresion": "COALESCE(NVL(P.PRS_C_POSTAL, ''), '0') AS PRS_C_POSTAL",
        "Metodo": ""
      },
      {
        "Columna": "PRS_TELEFONOS",
        "Expresion": "COALESCE(NVL(P.PRS_TELEFONOS, 0), '0') AS PRS_TELEFONOS",
        "Metodo": ""
      },
      {
        "Columna": "PRS_FAX",
        "Expresion": "COALESCE(NVL(P.PRS_FAX, 0), '0') AS PRS_FAX",
        "Metodo": ""
      },
      {
        "Columna": "PRS_ESTADO",
        "Expresion": "COALESCE(NVL(P.PRS_ESTADO, ''), '0') AS PRS_ESTADO",
        "Metodo": ""
      },
      {
        "Columna": "PRS_PISO",
        "Expresion": "COALESCE(NVL(P.PRS_PISO, 0), '0') AS PRS_PISO",
        "Metodo": ""
      },
      {
        "Columna": "CLL_NOMBRE",
        "Expresion": "TRIM(COALESCE(NVL(C.CLL_NOMBRE, ''), '0')) AS PRS_DIRECCION",
        "Metodo": ""
      },
      {
        "Columna": "PRS_LOCALIDAD",
        "Expresion": "",
        "Metodo": ""
      },
      {
        "Columna": "PRS_DIRECCION",
        "Expresion": "COALESCE(NVL(DECODE(P.PRS_BIS, 'N', ' ', P.PRS_BIS), ' '), '0') AS PRS_BIS, COALESCE(NVL(P.PRS_NRO, 0), '0') AS PRS_NRO, COALESCE(NVL(P.PRS_TORRE, ' '), '0') AS PRS_TORRE, COALESCE(NVL(P.PRS_PISO, 0), '0') AS PRS_PISO, COALESCE(NVL(P.PRS_DEPTO, ''), '0') AS PRS_DEPTO, COALESCE(NVL(P.PRS_C_POSTAL, ''), '0') AS PRS_C_POSTAL",
        "Metodo": ""
      }
    ]
  },
